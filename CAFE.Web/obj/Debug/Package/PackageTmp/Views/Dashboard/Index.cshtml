<md-content layout="column" flex ng-app="usersDashboard" class="admin-content" ng-controller="userDashboardController" style="height: 110%; margin-top: 20px; overflow: visible;" ng-cloak>
    <div class="container body-content">
        <div class="panel-group">
            @{
                if (ViewBag.isUsersAcceptanceNeed == true)
                {
                    <unaccepted-users-list></unaccepted-users-list>
                }
            }
            <div class="panel panel-default panel-margin-bottom">
                <div class="panel-heading">Access requests</div>
                <div class="panel-body">
                    <access-requests-list></access-requests-list>
                </div>
            </div>
            <div class="panel panel-default panel-margin-bottom">
                <div class="panel-heading">Files list</div>
                <div class="panel-body">
                    <files-list></files-list>
                </div>
            </div>
            <div class="panel panel-default panel-margin-bottom">
                <div class="panel-heading">Annotation Items</div>
                <div class="panel-body">
                    <annotation-items-list></annotation-items-list>
                </div>
            </div>
        </div>
    </div>
</md-content>

@section scripts
{
    @Scripts.Render("~/bundles/admin/angular")
    @Scripts.Render("~/bundles/users-dashboard")
}

@section styles
{
    @Styles.Render("~/bundles/users-dashboard/css")
}