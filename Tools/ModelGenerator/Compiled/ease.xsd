  <xs:simpleType name="SoilMorphologyTypeVocabulary">
    <xs:annotation>
      <xs:documentation xml:lang="en">
	The soil morphology according to the World Reference Base for Soil Resources
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Histosols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with thick organic layers (WRB, 2014).
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Anthrosols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with strong human influence. With long and intensive agricultural use
	    (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Technosols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with strong human influence. Containing significant ammount of artefacts
	    (WRB, 2014).
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Cryosols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with limitation to root growth. Permafrost-affected (WRB, 2014).
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Leptotols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with limitation to root growth. Thin or with many coarse fragments (WRB,
	    2014).
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Solonetz">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with limitation to root growth. With a high ammount of exchangeable Na
	    (WRB, 2014).
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Vertisols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with limitation to root growth. Alternating wet-dry
	    conditions, schrink-swell clays (WRB, 2014).
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Solonchaks">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with limitation to root growth. High concentration of soluble
	    salts (WRB, 2014).
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Gleysols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils distinguished by Fe/Al chemistry. Groundwater-affected, underwater and in
	    tidal areas (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Andosols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils distinguished by Fe/Al chemistry. Allophanes or Al-humus complexes (WRB,
	    2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Podzols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils distinguished by Fe/Al chemistry. Subsoil accumulation of humus and/or
	    oxides (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Plinthosols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils distinguished by Fe/Al chemistry. Accumulation and redistribution of Fe
	    (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Nitisols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils distinguished by Fe/Al chemistry. Low-activity clay, P fixation, many Fe
	    oxides, strongly structured (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Ferralsols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils distinguished by Fe/Al chemistry. Dominance of kaolinit and oxides (WRB,
	    2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Planosols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils distinguished by Fe/Al chemistry. Stagnating water, abrupt textural
	    difference (WRB, 2014).
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Stagnosols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils distinguished by Fe/Al chemistry. Stagnating water, structural difference
	    and/or moderate textural difference (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Chernozems">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Pronounced accumulation of organic matter in the mineral topsoil. Very dark
	    topsoil, secondary carbonates (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Kastanozems">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Pronounced accumulation of organic matter in the mineral topsoil. Dark topsoil,
	    secondary carbonates (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Phaeozems">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Pronounced accumulation of organic matter in the mineral topsoil. Dark topsoil,
	    no secondary carbonates (unless very deep), high base status (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Umbrisols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Pronounced accumulation of organic matter in the mineral topsoil. Dark topsoil,
	    low base status (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Durisols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Accumulation of moderately soluble salts or non-saline substances. Accumulation
	    of, and cementation by, secondary silica (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Gypsisols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Accumulation of moderately soluble salts or non-saline substances. Accumulation
	    of secondary gypsum (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Calcisols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Accumulation of moderately soluble salts or non-saline substances. Accumulation
	    of secondary carbonates (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Retisols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with clay enriched subsoil. Interfingering of coarser-textured, lighter
	    coloured material into a finer textured, stronger coloured layer (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Acrisols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with clay enriched subsoil. Low-activity clays, low base status (WRB,
	    2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Lixisols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with clay enriched subsoil. Low-activity clays, high base status (WRB,
	    2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Alisols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with clay enriched subsoil. High-activity clays, low base status (WRB,
	    2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Luvisols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with clay enriched subsoil. High-activity clays, high base status (WRB,
	    2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Cambisols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with little or no profile differentiation. Moderately developed (WRB,
	    2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Arenosols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with little or no profile differentiation. Sandy (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Fluvisols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with little or no profile differentiation. Stratified fluviatile, marine
	    and lacustrine sediments (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Regosols">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Soils with little or no profile differentiation. No significant profile
	    development (WRB, 2014)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

