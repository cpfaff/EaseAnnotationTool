  <xs:simpleType name="PedoBiomeTypeVocabulary">
    <xs:annotation>
      <xs:documentation xml:lang="en">
	A type of pedo biome
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Amphibiome">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Amphibiomes are pedobiomes with changing wet or temporarily flooded
	    soils.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Halobiome">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Halobiomes are pedobiomes with salty soils
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Helobiome">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Helobiomes are pedobiomes with soils containing a high amount of
	    humus (moor)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Hydrobiome">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Hydrobiomes are pedobiomes where the soil is continously under
	    water.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Lithobiome">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Lithobiomes are pedobiomes where the soil is characterized by
	    skelletal stone structures.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Peinobiome">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Peinobiomes are pedobiomes with nutrient-poor soils.
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Psammobiome">
	<xs:annotation>
	  <xs:documentation xml:lang="en">
	    Psammobiomes are pedobiomes whith sandy soils (e.g. dunes)
	  </xs:documentation>
	</xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

