            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="buildingType" type="common:BuildingType" minOccurs="0" maxOccurs="11">
                  <xs:annotation>
                    <xs:documentation xml:lang="en">building type</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Gebäudeart</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="livingCriteria" type="LivingCriteria" minOccurs="0">
