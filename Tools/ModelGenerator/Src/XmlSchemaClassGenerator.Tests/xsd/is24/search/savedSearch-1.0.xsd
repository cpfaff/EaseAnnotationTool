        </xs:annotation>
        <xs:sequence>
	      <xs:element name="rented" type="common:YesNoBothType" default="BOTH">
	        <xs:annotation>
	          <xs:documentation xml:lang="en">rented</xs:documentation>
	          <xs:documentation xml:lang="de-DE">vermietet</xs:documentation>
	        </xs:annotation>
	      </xs:element>
          <xs:element name="buildingProject" type="common:YesNotApplicableType" default="NOT_APPLICABLE"
                      minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">building project</xs:documentation>
              <xs:documentation xml:lang="de-DE">Neubau-Projekt</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

