        </xs:annotation>
        <xs:sequence>
	      <xs:element name="rented" type="common:YesNoBothType" default="BOTH">
	        <xs:annotation>
	          <xs:documentation xml:lang="en">rented</xs:documentation>
	          <xs:documentation xml:lang="de-DE">vermietet</xs:documentation>
	        </xs:annotation>
	      </xs:element>
          <xs:element name="constructionPhaseTypes" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">construction phase types</xs:documentation>
              <xs:documentation xml:lang="de-DE">Bauphasen</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="constructionPhaseType" type="common:ConstructionPhaseType" minOccurs="0"
                  maxOccurs="4">
                  <xs:annotation>
                    <xs:documentation xml:lang="en">construction phase type</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Bauphase</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="lodgerFlat" type="common:YesNotApplicableType"
             default="NOT_APPLICABLE" minOccurs="0">
