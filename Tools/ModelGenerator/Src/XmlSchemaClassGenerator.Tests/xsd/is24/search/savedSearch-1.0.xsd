              <xs:documentation xml:lang="en">internet connection types</xs:documentation>
              <xs:documentation xml:lang="de-DE">Internet Typen</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="internetConnectionType" type="common:InternetConnectionType" minOccurs="0"
                  maxOccurs="2">
                  <xs:annotation>
                    <xs:documentation xml:lang="en">internet connection type</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Internet Typ</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="petsAllowedTypes" minOccurs="0">
            <xs:annotation>
