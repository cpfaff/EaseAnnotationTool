              <xs:documentation xml:lang="de-DE">Rauchen erlaubt</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="flatMateGenderTypes" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">flat mate gender types</xs:documentation>
              <xs:documentation xml:lang="de-DE">Gesuchte Mitbewohner</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="flatMateGenderType" type="common:FlatMateGenderType" minOccurs="0" maxOccurs="2">
                  <xs:annotation>
                    <xs:documentation xml:lang="en">flat mate gender type</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Gesuchter Mitbewohner</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="flatShareSize" type="common:FlatShareSearchSize" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">flat share size</xs:documentation>
              <xs:documentation xml:lang="de-DE">WG-Größe</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="internetConnectionTypes" minOccurs="0">
