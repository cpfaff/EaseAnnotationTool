          </xs:element>
          <xs:element name="areaSize" type="common:AreaRange" minOccurs="0">
	        <xs:annotation>
	          <xs:documentation xml:lang="en">plot area</xs:documentation>
	          <xs:documentation xml:lang="de-DE">Grundstücksfläche</xs:documentation>
	        </xs:annotation>
	      </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="HouseRentQuery">
    <xs:annotation>
      <xs:documentation xml:lang="en">Search elements for the house rent realestate type.</xs:documentation>
      <xs:documentation xml:lang="de-DE">Sucheigenschaften für den Haus-Miete Immobilientyp.</xs:documentation>
    </xs:annotation>
