              <xs:documentation xml:lang="en">parking space</xs:documentation>
              <xs:documentation xml:lang="de-DE">Garage / Stellplatz</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="freeDuration" type="common:Number1.0Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">free duration</xs:documentation>
              <xs:documentation xml:lang="de-DE">frei für mindestens</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="freeInterval" type="common:DurationType" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">free interval</xs:documentation>
              <xs:documentation xml:lang="de-DE">frei für ... Tag / Woche / Monat</xs:documentation>
