      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="AbstractSearchQuery">
        <xs:annotation>
          <xs:documentation xml:lang="en">Query with all general informations.</xs:documentation>
          <xs:documentation xml:lang="de-DE">Suche mit allen generellen Informationen.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="shortTermAccommodationType" type="common:ShortTermAccommodationType" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">short term accommodation types</xs:documentation>
              <xs:documentation xml:lang="de-DE">Art der Unterkunft</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="startRentalDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">start rental date</xs:documentation>
              <xs:documentation xml:lang="de-DE">Bezugsfrei ab</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="price" type="common:PriceRange" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">total rent</xs:documentation>
              <xs:documentation xml:lang="de-DE">Gesamtmiete</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="rentInterval" type="common:DurationType" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">rent interval per day, week or month</xs:documentation>
              <xs:documentation xml:lang="de-DE">Miete pro Tag/Woche/Monat</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="furniture" type="common:YesNoBothType" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">has furniture</xs:documentation>
              <xs:documentation xml:lang="de-DE">Möbliert</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="smokingAllowed" type="common:YesNoBothType" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">smoker</xs:documentation>
              <xs:documentation xml:lang="de-DE">Rauchen erlaubt</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="minNumberOfPersons" type="common:Number1.0Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">minimum numbers of persons</xs:documentation>
              <xs:documentation xml:lang="de-DE">Belegung mindestens für ... Personen</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="cellar" type="common:YesNotApplicableType" default="NOT_APPLICABLE" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">cellar</xs:documentation>
              <xs:documentation xml:lang="de-DE">Keller</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="guestToilet" type="common:YesNotApplicableType" default="NOT_APPLICABLE"
            minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">guest toilet</xs:documentation>
              <xs:documentation xml:lang="de-DE">Gästetoilette</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="parking" type="common:YesNotApplicableType" default="NOT_APPLICABLE"
            minOccurs="0">
