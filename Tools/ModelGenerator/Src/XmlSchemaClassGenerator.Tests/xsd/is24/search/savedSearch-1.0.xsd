<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!--
    Common schema which will be used within the search API context, so be very careful with incompatible changes. 
    In case of doubt please contact DEV2-API for support.
-->
<xs:schema
    xmlns="http://rest.immobilienscout24.de/schema/search/savedSearch/1.0"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:common="http://rest.immobilienscout24.de/schema/common/1.0"
    targetNamespace="http://rest.immobilienscout24.de/schema/search/savedSearch/1.0"
    elementFormDefault="unqualified"
    attributeFormDefault="unqualified"
    version="1.0">

  <xs:import namespace="http://rest.immobilienscout24.de/schema/search/common/1.0" 
  	schemaLocation="common-1.0.xsd" />
  <xs:import namespace="http://rest.immobilienscout24.de/schema/common/1.0"/>

  <xs:element name="savedSearches">
    <xs:annotation>
      <xs:documentation xml:lang="en">Represents all persisted searches of an user.</xs:documentation>
      <xs:documentation xml:lang="de-DE">Repräsentiert alle gespeicherten Gesuche eines Benutzers.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="savedSearch" type="SavedSearch" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="savedSearch" type="SavedSearch">
    <xs:annotation>
      <xs:documentation xml:lang="en">Saved search with all informations.</xs:documentation>
      <xs:documentation xml:lang="de-DE">Ein gespeichertes Gesuch.</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="SavedSearch">
    <xs:annotation>
      <xs:documentation xml:lang="en">Saved search with all informations</xs:documentation>
      <xs:documentation xml:lang="de-DE">Gesuch mit allen Informationen.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="searchQuery" type="AbstractSearchQuery">
        <xs:annotation>
          <xs:documentation xml:lang="en">Query with all informations.</xs:documentation>
          <xs:documentation xml:lang="de-DE">Suche mit allen Informationen.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="fulfillmentActivated" type="xs:boolean" minOccurs="0" default="false">
        <xs:annotation>
          <xs:documentation xml:lang="en">Indicates whether the fulfillment process is acticated.
          </xs:documentation>
          <xs:documentation xml:lang="de-DE">Gibt an of der Benachrichtungsservice aktiviert ist.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="name" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Name of this saved search.</xs:documentation>
          <xs:documentation xml:lang="de-DE">Name der Gesuchsspeicherung.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="common:TextField">
            <xs:maxLength value="60" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:all>
    <xs:attributeGroup ref="common:Reference" />
  </xs:complexType>


<!-- Query definitions -->


  <xs:complexType name="AbstractSearchQuery" abstract="true">
    <xs:annotation>
      <xs:documentation xml:lang="en">Query with all general informations.</xs:documentation>
      <xs:documentation xml:lang="de-DE">Suche mit allen generellen Informationen.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="geoCriteria" type="AbstractGeoCriteria">
        <xs:annotation>
          <xs:documentation xml:lang="en">Geoinformations of the underlying search.</xs:documentation>
          <xs:documentation xml:lang="de-DE">Geoinformationen der zugrundeliegenden Suche.</xs:documentation>
        </xs:annotation>
      </xs:element>
       <xs:element name="fullText" type="common:TextField" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">query for full text search.</xs:documentation>
          <xs:documentation xml:lang="de-DE">Wert für Volltextsuche.</xs:documentation>
        </xs:annotation>
      </xs:element>        
       <xs:element name="strict" type="xs:boolean" minOccurs="0" default="false">
        <xs:annotation>
          <xs:documentation xml:lang="en">output as URIs</xs:documentation>
          <xs:documentation xml:lang="de-DE">Ausgabe als URIs</xs:documentation>
        </xs:annotation>       
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ApartmentQuery" abstract="true">
    <xs:annotation>
      <xs:documentation xml:lang="en">Query with all general apartment informations.</xs:documentation>
      <xs:documentation xml:lang="de-DE">Suche mit allen generellen Wohninformationen.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="AbstractSearchQuery">
        <xs:annotation>
          <xs:documentation xml:lang="en">Query with all general informations.</xs:documentation>
          <xs:documentation xml:lang="de-DE">Suche mit allen generellen Informationen.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="price" type="common:PriceRange" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">price rent / buy</xs:documentation>
              <xs:documentation xml:lang="de-DE">Preis (Miete / Kaufpreis)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="heatingTypes" type="HeatingTypes" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">heating types</xs:documentation>
              <xs:documentation xml:lang="de-DE">Heizungsarttypen</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="apartmentTypes" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">apartment types</xs:documentation>
              <xs:documentation xml:lang="de-DE">Wohntypen</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="apartmentType" type="common:ApartmentType" minOccurs="0" maxOccurs="10">
                  <xs:annotation>
                    <xs:documentation xml:lang="en">apartment type</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Wohntyp</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="livingCriteria" type="LivingCriteria" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">criterias for all living real estates.</xs:documentation>
              <xs:documentation xml:lang="de-DE">Kriterien für alle Wohnimmobilien.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="freeOfCourtageOnly" type="common:YesNotApplicableType"
             default="NOT_APPLICABLE" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">free of courtage only</xs:documentation>
              <xs:documentation xml:lang="de-DE">Nur provisionsfreie Objekte</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ApartmentRentQuery">
    <xs:annotation>
      <xs:documentation xml:lang="en">Search elements for the apartment rent realestate type.
      </xs:documentation>
      <xs:documentation xml:lang="de-DE">Sucheigenschaften für den Wohnung-Miete Immobilientyp.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ApartmentQuery">
        <xs:annotation>
          <xs:documentation xml:lang="en">Query with all general apartment informations.</xs:documentation>
          <xs:documentation xml:lang="de-DE">Suche mit allen generellen Wohninformationen.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="promotion" type="common:YesNoBothType" default="BOTH"
            minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">certificate of eligibility needed</xs:documentation>
              <xs:documentation xml:lang="de-DE">Wohnberechtigungsschein erforderlich</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ApartmentBuyQuery">
    <xs:annotation>
      <xs:documentation xml:lang="en">Search elements for the apartment buy realestate type.
      </xs:documentation>
      <xs:documentation xml:lang="de-DE">Sucheigenschaften für den Wohnung-Kauf Immobilientyp.
      </xs:documentation>
    </xs:annotation>
