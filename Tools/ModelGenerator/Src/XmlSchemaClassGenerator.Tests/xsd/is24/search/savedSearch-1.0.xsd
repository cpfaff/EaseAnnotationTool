              <xs:documentation xml:lang="de-DE">Nur provisionsfreie Objekte</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="FlatShareRoomQuery">
    <xs:annotation>
