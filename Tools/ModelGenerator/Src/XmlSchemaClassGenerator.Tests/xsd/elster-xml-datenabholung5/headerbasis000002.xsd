		<xs:annotation>
			<xs:documentation xml:lang="DE">siehe Elster Developers Toolkit </xs:documentation>
			<xs:documentation xml:lang="DE">13-stellig numerisch </xs:documentation>
			<xs:documentation xml:lang="DE">Bundesfinanzamtsnummer + 0 + Bezirk +  Unterscheidungsnr. + Pruefziffer</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterheaderbasis:BaseStringSType">
			<xs:length value="13"/>
