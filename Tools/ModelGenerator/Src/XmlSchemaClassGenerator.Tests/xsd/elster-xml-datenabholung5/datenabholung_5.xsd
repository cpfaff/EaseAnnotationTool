:simpleType>		</xs:attribute>		<xs:attribute name="von" type="elster:MyDateCType" use="optional"/>		<xs:attribute name="bis" type="elster:MyDateCType" use="optional"/>		<xs:attribute name="veranlagungsjahr" type="xs:gYear" use="optional"/>		<xs:attribute name="steuerart" type="xs:string" use="optional"/>		<xs:attribute name="finanzamt" use="optional">			<xs:simpleType>				<xs:restriction base="xs:integer">					<xs:minInclusive value="1000"/>					<xs:maxExclusive value="9999"/>				</xs:restriction>			</xs:simpleType>		</xs:attribute>		<xs:attribute name="bundesland" use="optional">			<xs:simpleType>				<xs:restriction base="xs:string">					<xs:enumeration value="BW"/>					<xs:enumeration value="BY"/>					<xs:enumeration value="BE"/>					<xs:enumeration value="BB"/>					<xs:enumeration value="HB"/>					<xs:enumeration value="HH"/>					<xs:enumeration value="HE"/>					<xs:enumeration value="MV"/>					<xs:enumeration value="ND"/>					<xs:enumeration value="NW"/>					<xs:enumeration value="RP"/>					<xs:enumeration value="SL"/>					<xs:enumeration value="SN"/>					<xs:enumeration value="ST"/>					<xs:enumeration value="SH"/>					<xs:enumeration value="TH"/>				</xs:restriction>			</xs:simpleType>		</xs:attribute>		<xs:attribute name="max" use="optional" default="100">			<xs:simpleType>				<xs:restriction base="xs:integer">					<xs:minInclusive value="1"/>					<xs:maxInclusive value="10000"/>				</xs:restriction>			</xs:simpleType>		</xs:attribute>		<xs:attribute name="anforderungsticket" type="xs:string" use="optional"/>		<xs:attribute name="agstnr" type="xs:string" use="optional"/>		<xs:attribute name="schemaversion" type="xs:string" use="optional"/>		<xs:attribute name="lieferticket" type="xs:string" use="optional"/>		<xs:attribute name="idnr" type="elster:IdNrSType" use="optional"/>		<xs:attribute name="belegart" use="optional" type="xs:string"/>	</xs:complexType>	<xs:complexType name="AbholungCType">		<xs:sequence>